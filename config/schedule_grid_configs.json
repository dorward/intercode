[
  {
    "key": "con_schedule",
    "basename": "/events/schedule",
    "title": "Con schedule",
    "classifyEventsBy": "category",
    "showSignedUp": true,
    "showSignupStatusBadge": true,
    "showPersonalFilters": true,
    "groupEventsBy": "category",
    "categoryGroups": [
      {
        "id": "catchAll",
        "match": [{ "allRemaining": true }]
      },
      {
        "id": "social",
        "match": [{ "categoryName": "Meetup" }, { "categoryName": "Social event" }]
      },
      {
        "id": "games",
        "flexGrow": 1,
        "match": [{ "categoryName": "Larp" }, { "categoryName": "Tabletop RPG" }, { "categoryName": "Board game" }]
      },
      {
        "id": "games",
        "flexGrow": 1,
        "match": [{ "categoryName": "Larp" }, { "categoryName": "Tabletop RPG" }, { "categoryName": "Board game" }]
      },
      {
        "id": "conServices",
        "match": [{ "categoryName": "Con services" }]
      },
      {
        "id": "volunteerEvents",
        "match": [{ "categoryName": "Volunteer event" }]
      }
    ],
    "filterEmptyGroups": true,
    "legends": [
      { "type": "category" }
    ]
  },
  {
    "key": "con_schedule_by_room",
    "basename": "/events/schedule_by_room",
    "title": "Con schedule by room",
    "classifyEventsBy": "category",
    "showSignupStatusBadge": true,
    "showSignedUp": true,
    "groupEventsBy": "room",
    "filterEmptyGroups": true,
    "legends": [
      {
        "type": "text",
        "text": "Events that use multiple rooms will appear multiple times on this schedule."
      },
      { "type": "category" }
    ]
  },
  {
    "key": "schedule_with_counts",
    "basename": "/events/schedule_with_counts",
    "title": "Schedule with counts",
    "classifyEventsBy": "fullness",
    "showExtendedCounts": true,
    "groupEventsBy": "category",
    "categoryGroups": [
      {
        "id": "catchAll",
        "match": [{ "allRemaining": true }]
      },
      {
        "id": "games",
        "flexGrow": 1,
        "match": [{ "categoryName": "Larp" }, { "categoryName": "Tabletop RPG" }, { "categoryName": "Board game" }]
      },
      {
        "id": "conServices",
        "match": [{ "categoryName": "Con services" }]
      },
      {
        "id": "volunteerEvents",
        "match": [{ "categoryName": "Volunteer event" }]
      }
    ],
    "filterEmptyGroups": true,
